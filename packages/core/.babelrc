{
    "env": {
      "production": {
        "presets": [
          "@babel/env",
          [
            "@babel/preset-react",
            {
              "runtime": "automatic"
            }
          ],
          "@babel/preset-typescript"
        ],
        "plugins": ["@babel/plugin-proposal-class-properties"]
      },
      "development": {
        "presets": [
          "@babel/env",
          [
            "@babel/preset-react",
            {
              "runtime": "automatic"
            }
          ],
          "@babel/preset-typescript"
        ],
        "plugins": ["@babel/plugin-proposal-class-properties"]
      },
      "test": {
        "plugins": ["@babel/plugin-transform-runtime"],
        "presets": [
          "@babel/preset-env",
          "@babel/preset-typescript",
          "@babel/preset-react"
        ]
      }
    }
  }
  